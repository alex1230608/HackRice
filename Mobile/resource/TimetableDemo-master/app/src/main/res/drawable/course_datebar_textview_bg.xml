<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <!--shape 节点配置的是图形的形式，主要包括方形、圆形等-->
        <shape>
            <!-- 实心 -->
            <solid android:color="#FFFFFF" />
            <!-- 描边 -->
            <stroke
                android:width="1dp"
                android:color="#a8abad" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:right="1dp">
        <shape>
            <solid android:color="#FFFFFF" />
            <stroke
                android:width="1dp"
                android:color="#ffffff" />
        </shape>
    </item>
</layer-list>
